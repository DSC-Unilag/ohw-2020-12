{
  "name": "recipe-saver",
  "version": "1.0.0",
  "description": "An app where you can save your recipes, view other recipes and star your favorite recipe",
  "main": "dist/app.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "tsc && node dist/server.js",
    "build": "tsc",
    "start:watch": "nodemon",
    "test": "jest",
    "jest:init": "jest --init"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Olusamimaths/ohw-2020-12.git"
  },
  "keywords": [
    "recipe-saver",
    "node",
    "typescript"
  ],
  "author": "Olusola Samuel",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Olusamimaths/ohw-2020-12/issues"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm run dev",
    "ext": "ts"
  },
  "engines": {
    "node": "10.x"
  },
  "homepage": "https://github.com/Olusamimaths/ohw-2020-12#readme",
  "devDependencies": {
    "@babel/core": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-typescript": "^7.10.4",
    "@types/bcrypt": "^3.0.0",
    "@types/body-parser": "^1.19.0",
    "@types/cors": "^2.8.6",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.7",
    "@types/express-validator": "^3.0.0",
    "@types/jest": "^26.0.8",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mongoose": "^5.7.34",
    "@types/multer": "^1.4.3",
    "@types/node": "^14.0.27",
    "@types/supertest": "^2.0.10",
    "babel-jest": "^26.2.2",
    "jest": "^26.2.2",
    "nodemon": "^2.0.4",
    "supertest": "^4.0.2",
    "ts-jest": "^26.1.4",
    "tslint": "^6.1.2",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "cloudinary": "^1.22.0",
    "cors": "^2.8.5",
    "datauri": "^3.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.6.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.26",
    "multer": "^1.4.2"
  }
}
